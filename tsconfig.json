{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es6",
    "allowJs": true,
    "noImplicitReturns": false
  },
  "exclude": [
    "dist/**/*",
    "release/**/*",
    "webpack.main.ext.js",
    "webpack.renderer.ext.js",
    "babel.config.js"
  ]
}
